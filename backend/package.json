{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "dev:start": "ts-node-dev --respawn --pretty --transpile-only -r tsconfig-paths/register src/index.ts",
    "dev:tsc": "tsc -w",
    "dev": "concurrently --kill-others \"yarn dev:start\" \"yarn dev:tsc\"",
    "build": "tsc && tsc-alias",
    "migrate": "npx prisma migrate dev --name init"
  },
  "dependencies": {
    "@prisma/client": "^4.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.17",
    "prisma": "^4.8.0",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4"
  }
}
